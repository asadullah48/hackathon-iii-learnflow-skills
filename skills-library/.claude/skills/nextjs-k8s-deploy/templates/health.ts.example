import type { NextApiRequest, NextApiResponse } from 'next'

export default async function handler(
  req: NextApiRequest,
  res: NextApiResponse
) {
  // Basic health check
  res.status(200).json({ 
    status: 'healthy',
    timestamp: new Date().toISOString()
  })
}
